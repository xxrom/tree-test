    "@types/testing-library__react": "^10.2.0",
    "@types/uuid": "^8.3.4",
    "jest-fetch-mock": "^3.0.3",
    "autoprefixer": "^10.4.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.12",
